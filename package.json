    "gsap": "^3.13.0",
    "lenis": "^1.3.8",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "react-spring": "^10.0.1",
    "tailwindcss": "^4.1.11",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
